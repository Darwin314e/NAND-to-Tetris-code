/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel ,out=Nsel);
    And(a=a ,b=Nsel ,out=o1);
    And(a=sel ,b=b ,out=o2);
    Or(a=o1 ,b=o2 ,out=out);
    ////Sadly I want able to do this one without looking up the circuit design.
}